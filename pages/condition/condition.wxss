.main-box2{
  padding-bottom:140rpx; 
}
.time {
  width: 50%;
  text-align: center;
  color: #999;
  font-size: 32rpx;
  height: 120rpx;
  line-height: 120rpx;
  position: relative;
}

.startTime::after {
  content: "";
  width: 1rpx;
  background: #ddd;
  height: 120rpx;
  position: absolute;
  right: 0;
  top: 0;
}

.timebox {
  position: relative;
}

.timebox::after {
  content: "";
  height: 1rpx;
  background: #ddd;
  width: 100%;
  position: absolute;
  right: 0;
  top: 140rpx;
}

.trading-area {
  height: 48rpx;
  line-height: 48rpx;
  margin-top: 60rpx;
}

.lists {
  flex-wrap: wrap;
  /* justify-content: space-between; */
}

.item {
  position: relative;
  padding-left: 48rpx;
  /* width: 26%; */
  margin: 42rpx 40rpx 0 0rpx;
  font-size: 28rpx;
}

.item::before {
  position: absolute;
  width: 36rpx;
  height: 36rpx;
  border: 1rpx solid #666;
  border-radius: 6rpx;
  content: "";
  left: 0rpx;
  top: -1rpx;
}

.active::after {
  position: absolute;
  content: "";
  display: inline-block;
  background: url("data:image/png;base64,");
  width: 34rpx;
  height: 34rpx;
  background-size: cover;
  left: -0rpx;
  top: 0;
}

.price-box {
  border-top: 1rpx solid #ddd;
  margin-top: 42rpx;
  padding: 26rpx 0 42rpx;
}

.price {
  width: 176rpx;
  height: 56rpx;
  line-height: 56rpx;
  color: #666;
  border: 1rpx solid #666;
  font-size: 26rpx;
  text-align: center;
}

.line {
  height: 1rpx;
  width: 118rpx;
  margin: 0 20rpx;
  background: #666;
  margin-top: 28rpx; 
}

.btn-box {
  position: fixed;
  bottom: 0;
  width: 100%;
  height: 116rpx;
  left: 0;
  font-size: 32rpx;
  padding: 0 30rpx;
  box-sizing: border-box;
  border-top: 1rpx solid #ddd;
  z-index: 999;
  background: #fff;
}

.reset, .serch {
  height: 72rpx;
  width: 264rpx;
  color: #f64;
  line-height: 72rpx;
  border-radius: 0rpx;
  text-align: center;
  margin-top: 21rpx;
  border: 1rpx solid #f64;
}

.serch {
  height: 74rpx;
  line-height: 74rpx;
  background: #f64;
  color: #fff;
  width: 374rpx;
  border: none;
}
.total-day{
  position: absolute;
  width: 100rpx;
  height: 30rpx;
  font-size: 20rpx;
  line-height: 30rpx;
  color: #666;
  border-radius: 16rpx;
  border: 1rpx solid #999;
  text-align: center;
  left: 50%;
  top: 50%;
  margin-left: -50rpx;
margin-top: -16rpx;
background: #fff;
}
