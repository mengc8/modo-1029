.container>view{
  width: 100%;
  box-sizing: border-box;
  border-bottom: 1rpx solid #ddd;
  color: #666;
}
.bind-phone,.header{
  padding: 40rpx 30rpx;
}
.header,.haddle{
  display: flex;
  justify-content: space-between;
}
.lists{
  padding: 20rpx 30rpx;
}
.lists-item{
  position: relative;
  margin-bottom: 20rpx;
}
.lists-item.active{
    padding-left: 60rpx;
}
.edit-box{
  margin-right: 20rpx;
}
.plus,.edit,.delete{
  width: 32rpx;
  height: 32rpx;
  background: url(data:image/png;base64,);
  background-size: cover;
  vertical-align: middle;
  margin: -8rpx 10rpx 0;
}
.edit{
  background: url(data:image/png;base64,);
  background-size: cover;
}
.delete{
  display: none;
  background: url(data:image/png;base64,);
  background-size: cover;
  position: absolute;
  left: 0;
  top: 30rpx;
  width: 36rpx;
  height: 36rpx;
}
.delete.active{
  display: inline-block;
}