  color: #333;
}

.photo-box {
  float: right;
  width: 148rpx;
  height: 148rpx;
  background: #e1e1e1;
  text-align: center;
}

.btn {
  position: fixed;
  width: 100%;
  height: 116rpx;
  line-height: 116rpx;
  background: #f64;
  color: #fff;
  font-size: 34rpx;
  left: 0;
  bottom: 0;
  border-radius: 0;
}
