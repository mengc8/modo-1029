.box{
  color: #666;
} 
.order{
padding: 20rpx 30rpx 30rpx;
font-size: 30rpx;
border-bottom: 14rpx solid #f2f2f2;
}
.room-img{
  width: 286rpx;
  height: 216rpx;
  float: left;
}
.room-info{
  margin-left: 300rpx;
}
.branch{
  margin-bottom: 24rpx;
}
.type,.time{
  font-size: 22rpx;
}
.money{
  font-size: 40rpx;
}
#money{
  padding-top: 40rpx;
}
.discribe{
  float: left;
}
.star-box{
  margin-left: 286rpx;
  display: flex;
  margin-top: 60rpx;
}
.star{
  width: 45rpx;
  height: 43rpx;
  margin-right: 38rpx;
  background: url(data:image/png;base64,);
  background-size: cover;
}
.star.active{
  background: url(data:image/png;base64,);
  background-size: cover;
}
.score-item{
  display: flex;
  justify-content: space-between;
  margin-top: 30rpx;
}
.score-item-name{
  width: 100rpx;
}
.star-box1{
    display: flex;
}
.sub-score{
  padding: 44rpx 30rpx;
}
.sub-score-title{
  margin-bottom: 68rpx;
}
.note{
  padding: 40rpx 30rpx;
}
textarea{
  width: 508rpx;
  height: 148rpx;
  background: #e1e1e1;
  display: inline-block;
  font-size: 28rpx;
  padding: 30rpx;
  box-sizing: border-box;
  color: #333;
}
.photo-box{
  float: right;
  width: 148rpx;
  height: 148rpx;
  background: #e1e1e1;
  text-align: center;
}
.img{
  margin-top: 50rpx;
  width: 60rpx;
  height: 47rpx;
  background: url(data:image/png;base64,);
  background-size: cover;
}
.btn-box{
  height: 116rpx;
  background: #fff;
  border-top: 1rpx solid #ddd;
  position: fixed;
  bottom: 0;
  width: 100%;
    line-height: 116rpx;
}
.publish{
  float: right;
  width: 220rpx;
  background: linear-gradient(to right bottom,#ff0000,#ff6644);
  color: #fff;
  height: 100%;
  border-radius: 0;
  line-height: 116rpx;
}
.hidename{
  width: 43rpx;
  height: 43rpx;
  background: url(data:image/png;base64,);
  background-size: cover;
  vertical-align: middle;
  margin: -10rpx 10rpx 0 30rpx;
}
.hidename.active{
  background: url(data:image/png;base64,);
  background-size: cover;
}
.sub-score-subtitle{
  font-size: 30rpx;
}
.right-btn{
  float: left;
}
.img-lists{
  display: flex;
  padding: 0 30rpx 160rpx;
}
.img-item{
  height: 210rpx;
  width: 210rpx;
  margin-left: 30rpx;
}
.img-item:nth-of-type(1){
   margin-left: 0rpx;
}