.header{
  height: 178rpx;
  background: #f6f6f6;
  padding: 52rpx 30rpx 0rpx;
  box-sizing: border-box;
  font-size: 36rpx;
}
.total{
  font-size: 28rpx;
  color: #999;
  margin-top: 6rpx;
}
.total view{
  margin-right: 40rpx;
}
.icon-down,.icon-up{
  width: 14rpx;
  height: 14rpx;
  border-top: 1rpx solid #333;
  border-right: 1rpx solid #333;
  transform: rotate(135deg);
  margin-top: -16rpx;
  vertical-align: middle;
}
.icon-up{
  transform: rotate(-45deg);
  margin-top: -6rpx;
}
picker{
  display: inline-block;
  margin-right: 10rpx;
}
.picker{
  display: inline-block;
  margin-right: 10rpx;
}
.lists{
  padding:0 30rpx;
}
.item{
  padding: 40rpx 0;
  font-size: 32rpx;
  box-sizing: border-box;
  position: relative;
}
.icon{
  width: 74rpx;
  height: 74rpx;
  position: absolute;
  background: url(data:image/png;base64,);
  background-size: cover;
  vertical-align: middle;
}
.icont2{
  background: url(data:image/png;base64,);
  background-size: cover;
}
.right-info{
  margin-left: 90rpx;
}
.time{
  font-size: 24rpx;
  color: #999;
}
.number{
  font-size: 36rpx;
  color: #f3565d;
}