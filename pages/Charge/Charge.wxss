.box {
  padding: 20rpx 30rpx;
  font-size: 30rpx;
  color: #999;
}

.title {
  height: 96rpx;
  line-height: 96rpx;
  color: #999;
}

.lists {
  flex-wrap: wrap;
}

.item {
  width: 320rpx;
  height: 82rpx;
  border-radius: 14rpx;
  border: 1rpx solid #999;
  margin-bottom: 18rpx;
  font-size: 28rpx;
  line-height: 82rpx;
  text-align: center;
  position: relative;
  color: #333;
}

.item.active {
  border: 1rpx solid #f64;
}

.item::after {
  width: 30rpx;
  height: 24rpx;
  position: absolute;
  display: inline-block;
  content: "";
  background: url(data:image/png;base64,);
  background-size: cover;
  right: 0;
  bottom: 0;
}

.active::after {
  background: url(data:image/png;base64,);
  background-size: cover;
}

.number-box,.money-box,.rule{
  padding: 20rpx 0rpx;
}

.number-haddler>view {
  border: 1rpx solid #333;
  width: 44rpx;
  height: 46rpx;
  margin: 0 2rpx;
  text-align: center;
  line-height: 46rpx;
  font-size: 28rpx;
  color: #333;
}
.number-haddler>view.disable{
  border: 1rpx solid #d0d0d0;
  color: #999;
}
.number-haddler>.number{
  width: 70rpx;
}
.money-details{
  color: #bf78be;
}
.payment{
  height: 88rpx;
  width: 690rpx;
  background: #ff6644;
color: #fff;
border-radius: 8rpx;
line-height: 88rpx;
text-align: center;
margin: 40rpx 0;
}
.rule-item{
  margin-top: 20rpx;
  color: #666;
  font-size: 24rpx;
}
.phone{
  display: inline-block;
}