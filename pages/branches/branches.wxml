<view class='container'>
  <view class='city-box'>
    <view class='city-name'>搜索历史
      <view class="clear" bindtap='clearBranch'>清除</view>
    </view>
    <view class='branch-box'>
      <view class='branch-item' wx:if="{{history}}" wx:for="{{history}}" wx:key="ID" wx:for-item="item" bindtap='choseBranch' data-id='{{item.ID}}' data-name='{{item.BranchName}}'>
        {{item.BranchName}}
      </view>
    </view>
  </view>
  <view class='city-box' wx:for="{{lists}}" wx:key="index" wx:for-item="item">
    <text class='city-name'>{{item.City.Name}}</text>
    <view class='branch-box'>
      <view class='branch-item' wx:for="{{item.Branchs}}" wx:key="ID" wx:for-item="item" bindtap='choseBranch' data-id='{{item.ID}}' data-name='{{item.BranchName}}'>
        {{item.BranchName}}
      </view>
    </view>
  </view>

  <view class='unkown'>
    <view bindtap='phone' id="phone">
      <image id="unkown" src='data:image/png;base64,'></image>不知道住在哪？
    </view>
  </view>
</view>