.border1{
  border-bottom: 1rpx solid #ddd;
}
.img-slider{
  width: 100%;
  height: 380rpx;
}
.slide-image,swiper{
  height: 100%;
  width: 100%;
}

.position-box,.time-box{
  padding: 34rpx 30rpx;
  position: relative;
  text-align: left;
  width: 100%;
  box-sizing: border-box;
}
.icon-position{
  position: absolute;
  height: 38rpx;
  width: 37rpx;
  background: url("data:image/png;base64,");
  background-size: cover;
  top: 50rpx;
}
.icon-next{
  background: url("data:image/png;base64,");
  position: absolute;
  width: 16rpx;
  height: 24rpx;
  background-size: cover;
}
.position-next{
  right: 30rpx;
  top: 60rpx;
}
.position-title,.time-title{
text-indent: 60rpx;
}
.sub-position-title,.sub-time-title{
  color: #999;
  font-size: 22rpx;
  margin-top: 4rpx;
  text-indent: 60rpx;

}
.tip-box{
  display: flex;
  font-size: 24rpx;
  color: #999;
}
#area{
  margin-right: 40rpx;
}
.area,.icon-type{
  width: 25rpx;
  height: 25rpx;
}
.icon-type{
  width: 26rpx;
}
.area{
  background: url("data:image/png;base64,");
  background-size: cover;
}
.icon-type{
  background: url("data:image/png;base64,");
  background-size: cover;
}
.time-title,.sub-time-title{
  text-indent: 0;
}
.time-title{
  color: #666;
  margin-top: 4rpx;
}
.totle-time{
  float: right;
  padding-right: 60rpx;
}
/* //列表部分 */
.room-lists{
  width: 100%;
  padding-bottom: 100rpx;
}
.room-item{
  padding :18rpx 30rpx;
  box-sizing: border-box;
}
.room-img{
  float: left;
  width: 146rpx;
  height: 100rpx;
  border-radius: 20rpx;
  position: relative;
}
.room-pic{
  width: 100%;
  height: 100%;
  border-radius: 8rpx;
}
.scale-bg{
  display: inline-block;
  position: absolute;
  bottom: 0;
  right: 0rpx;
  height: 26rpx;
  width: 40rpx;
  background: rgba(0, 0, 0, .5);
  border-bottom-right-radius: 10rpx;
  border-top-left-radius: 10rpx;
}
.scale{
  background: url("data:image/png;base64,");
  width: 21rpx;
  height: 20rpx;
  background-size: cover;
  position: absolute;
  top: 4rpx;
  left: 10rpx;
}
.room-info{
  margin-left: 160rpx;
  color: #999;
  display: flex;
  justify-content: space-between;
}
.room-type{
  color: #666;
  margin: 10rpx 0 4rpx;
}
.room-note{
  font-size: 22rpx;
}
.price-box{
  margin: 4rpx 0 4rpx;
  font-size: 16rpx;
}
.room-price{
  font-size: 36rpx;
}
.booking-btn,.close-btn{
  background: #ff6644;
  height: 40rpx;
  width: 120rpx;
  line-height: 40rpx;
  color: #fff;
  font-size: 28rpx;
}
.close-btn{
  background: #eee;
  color: #333;
}
/* 弹出层 */
.chose-room{
  position: fixed;
  bottom: -1020rpx;
  height: 1020rpx;
  width: 100%;
  left: 0;
  background: #fff;
  border-top-left-radius: 20rpx;
  border-top-right-radius: 20rpx
}
.room-tip-box{
  display: flex;
}
.chose-room-type{
  padding: 38rpx 30rpx 26rpx;
  font-size: 28rpx;
  color: #666;
}
.close{
  float: right;
  color: #333;
  width: 60rpx;
  text-align: right;
}
.chose-room-slider{
  height: 394rpx;
}
.room-tip-box{
  display: flex;
flex-wrap: wrap;
color: #666;
font-size: 22rpx;
margin: 16rpx 30rpx;
border-top: 1rpx solid #ddd;
border-bottom: 1rpx solid #ddd;
padding: 12rpx 0;
}
.chose-room-tip{
  width: 50%;
  text-indent: 76rpx;
  line-height: 46rpx;
}
.chose-room-title{
  margin-right: 30rpx;
  color: #999;
}
.sheshi{
  font-size: 20rpx;
  color: #666;
  padding: 0rpx 84rpx;
  line-height: 34rpx;
  position: relative;
}
.sheshi-title{
  color: #999;
  position: absolute;
}
.sheshi-content{
  text-indent: 104rpx;
}
.get-price{
  height: 98rpx;
  line-height: 98rpx;
  border-radius: 0;
  background: linear-gradient(to right,#ff4d4d,#ff6644);
  color: #fff;
  position: absolute;
  bottom: 0;
  width: 100%;
  left: 0;
}