.container{
  color: #666;
  background: #f2f2f2;
  display: flex;
  justify-content: flex-start;
}
.order-nav-box{
  display: flex;
  flex-wrap: nowrap;
  width: 100%;
  text-align: center;
  background: #fff;
}
.order-nav{
width: 25%;
padding-bottom: 14rpx;
}
.order-nav.active{
  border-bottom:1rpx solid #ff6644;
}
.icon-all{
  background: url("data:image/png;base64,");
   background-size: cover; 
}
.icon-all.active{
  background: url("data:image/png;base64,");
  background-size: cover;
  width: 58rpx;
}
.icon-pay{
  background: url("data:image/png;base64,");
  background-size: cover; 
  width: 73rpx;
}
.icon-pay.active{
  background: url("data:image/png;base64,");
  background-size: cover;
}
.icon-come{
  background: url("data:image/png;base64,");
   background-size: cover; 
}
.icon-come.active{
  background: url("data:image/png;base64,");
  background-size: cover;
}
.order-comment{
  background: url("data:image/png;base64,");
  background-size: cover; 
}
.order-comment.active{
  background: url("data:image/png;base64,");
    background-size: cover; 
}
icon{
width: 76rpx;
height: 75rpx;
display: block;
margin: 14rpx auto 4rpx;
}
text.active{
  color: #ff6644;
}
text.num{
  display: inline-block;
  width: 26rpx;
  height: 26rpx;
  background: #ff6644;
  vertical-align: middle;
  margin-top: -8rpx;
  line-height: 26rpx;
  border-radius: 8rpx;
  color: #fff;
}
.lists{
  width: 100%;
}
.list-item{
  margin-top: 28rpx;
  background: #fff;
  padding: 30rpx 26rpx;
  box-sizing: border-box;
}
.list-header{
  display: flex;
  justify-content: space-between;
}
.branch-name{
  font-size: 36rpx;
  position: relative;
}
.icon-right{
width: 16rpx;
height: 24rpx;
background: url("data:image/png;base64,");
background-size: cover;
position: absolute;
top: 1rpx;
right: -30rpx;
}
.order-status{
  font-size: 32rpx;
  color: #ff6644;
  text-align: right;
}
.room-type,.room-during{
  line-height: 50rpx;
}
.room-type{
  margin-top: 70rpx;
}
.list-footer{
  display: flex;
  justify-content: space-between;
}
.haddle-box{
  display: flex;
}
.list-footer{
  border-top: 1rpx solid #ddd;
  padding-top: 24rpx;
  margin-top: 24rpx;
}
.haddle{
  padding: 6rpx 14rpx;
  border: 1rpx solid #ddd;
  font-size: 30rpx;
  border-radius: 14rpx;
  margin: 0 12rpx;
  box-sizing: border-box;
}
.haddle:hover{
  background: #ff6644;
  color: #fff;
}
.shengyu{
  color: #666;
}
#noorder{
  text-align: center;
  margin-top: 200rpx;
}
.noorder{
  background: url("data:image/png;base64,");
  width: 340rpx;
  height: 343rpx;
  background-size: cover;
}