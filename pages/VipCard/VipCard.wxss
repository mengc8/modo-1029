.img-box{
  width: 690rpx;
  height: 380rpx;
  margin: 30rpx;
  position: relative;
  text-align: center;
  color: #666;
}
.evip{
  height: 100%;
  width: 100%;
  position: absolute;
  top: 0;
  left: 0;
  z-index: -1;
}
.vip-type{
  font-size: 50rpx;
  margin: 92rpx 0 42rpx;
}
.vip-type-more{
  font-size: 24rpx;
  color: #ff6644;
  margin-top: 26rpx;
}
.vip-title{
  position: relative;
  text-align: center;
}
.sheng-ming{
  position: absolute;
  right: 70rpx;
  top: 10rpx;
  color: #999;
  font-size: 22rpx;
}
.sheng-ming::after{
  display: inline-block;
  content: "?";
  height: 22rpx;
  width: 22rpx;
  border: 1px solid #999;
  border-radius: 12rpx;
  position: absolute;
  right: -30rpx;
  top: 3rpx;
  line-height: 24rpx;
  font-weight: 600;
}
.te-quan{
  background: #ff6644;
  width: 170rpx;
  height: 48rpx;
  line-height: 48rpx;
  color: #fff;
  font-size: 28rpx;
}
.vip-title-box{
  display: flex;
  border: 1px solid transparent;
  padding:20rpx 0;
  border-bottom: 14rpx solid #f7f7f7;
}
.vip-title-item{
  width: 220rpx;
  text-align: center;
  margin-top: 30rpx;
  color: #999;
  font-size: 28rpx;
}
.vip-title-item:nth-of-type(3n+2){
  width: 310rpx;
}
.vip-img{
  width: 138rpx;
  height: 115rpx;
  display: block;
  margin: 0 auto 28rpx;
}
.more-box{
  padding-bottom: 150rpx;
}
.more-item{
  padding: 25rpx 30rpx 10rpx;
}
.more-item:nth-of-type(n+2){
  border-top: 1px solid #ddd;
}
.more-title{
  font-size: 30rpx;
  color: #666;
  font-weight: 600;
  line-height: 80rpx;
}
.more-content{
  color: #999;
  font-size: 22rpx;
  line-height: 40rpx;
}
.buy-btn{
  position: fixed;
  width: 100%;
  height: 116rpx;
  background: rgba(0, 0, 0, 0.7);
  color: #fff;
  font-size: 38rpx;
  bottom: 0;
  left: 0;
  border-radius: 0;
  line-height: 116rpx;
}
navigator {
  display: inline-block;
  margin: 0 auto;
}