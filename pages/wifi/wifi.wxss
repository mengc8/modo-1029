.box{
  text-align: left;
  line-height: 66rpx;
  border-bottom: 1rpx solid #eee;
  padding: 50rpx 0;
  font-size: 30rpx;
}
.title{
  color: #999;
  display: inline-block;
  width: 375rpx;
  text-align: right;
  box-sizing: content-box;

}
.wifi{
  width: 519rpx;
  height: 277rpx;
  margin: 176rpx 115rpx;
}
#picker{
  display: inline-block;
  border: 1rpx solid #333;
  height: 46rpx;
  width: 150rpx;
  line-height: 46rpx;
  text-align: center;
}
.icon-down,.icon-up{
  width: 24rpx;
  height: 14rpx;
  background: url(data:image/png;base64,);
  background-size: cover;
  vertical-align: middle;
}
.icon-up{
  transform: rotate(180deg);
}
