.box{
  position: absolute;
  height: 100%;
  width: 100%;
  left: 0;
  top: 0;
  overflow-y: auto;
  background: #e9e9e9;
 
}
.header{
  padding: 52rpx 30rpx;
  color: #333;
  font-size: 28rpx;
  border-bottom: 20rpx solid #e9e9e9;
  background: #fff;
}
.branch-name{
  font-size: 34rpx;
}
.type{
  margin: 10rpx 0 20rpx;
  font-size: 26rpx;
}
.payment-item{
  padding: 0 30rpx;
  font-size: 28rpx;
    border-bottom: 20rpx solid #e9e9e9;
     background: #fff;
}
.item-index{
  font-size: 30rpx;
  /* font-weight: bold; */
}
.item-title{
height: 94rpx;
line-height: 94rpx;
border-bottom: 1rpx solid #e9e9e9;
font-size: 26rpx;
}
.item-content{
  padding: 28rpx 0;
  color: #666;

}
.item-time{
  margin: 10rpx 0;
}
.payed{
  color: #07c904;
}
.wait-icon,.payed-icon{
  width: 30rpx;
  height: 30rpx;
  vertical-align: middle;
  margin: -6rpx 6rpx 0;
}
.payed-icon{
  background: url(data:image/png;base64,);
  background-size: cover;
}
.wait-icon{
  background: url(data:image/png;base64,);
    background-size: cover;
}
.payment-btn{
  font-size: 24rpx;
  color: #fff;
  width: 120rpx;
  height: 48rpx;
  border-radius: 8rpx;
  background: #ff6644;
  float: right;
  text-align: center;
  line-height: 48rpx;
  margin-top: -6rpx;
}