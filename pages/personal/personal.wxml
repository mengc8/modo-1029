<view class="container">
  <view class='person-header border-bottom15 width100'>
    <view wx:if="{{isLogin}}">
      <view class='login-or-register'>登录、注册</view>
      <text class='person-note'>登录后立享会员预订优惠</text>
    </view>
    <view wx:else>
      <image class='head-img' src='{{userInfo.avatarUrl}}'></image>
      <view class='person-note font24'>{{userInfo.nickName}}</view>
    </view>
  </view>
  <view class='sub-header width100 border-bottom15'>
    <view class='item'>
      <image class='sub-img' src='data:image/png;base64,'></image>
      <text class='img-info'>全部订单</text>
    </view>
    <view class='item'>
      <image class='sub-img' src='data:image/png;base64,'></image>
      <text class='img-info'>待支付</text>
    </view>
    <view class='item'>
      <image class='sub-img' src='data:image/png;base64,'></image>
      <text class='img-info'>待入住</text>
    </view>
    <view class='item'>
      <image class='sub-img' src='data:image/png;base64,'></image>
      <text class='img-info'>待评价</text>
    </view>

  </view>
  <view class='squared-box width100'>
    <view class='squared-title'>必备工具</view>
    <view class='squared-item'>
    <navigator url='../VipCard/VipCard'>
      <image class='squared-img' src='data:image/png;base64,'></image>
      <text class='squared-info'>我的会员</text>
      </navigator>
    </view>
    <view class='squared-item'>

      <image class='squared-img' src='data:image/png;base64,'></image>
      <text class='squared-info'>领取积分</text>
    </view>
    <view class='squared-item'>
      <navigator url='../wifi/wifi'>
        <image class='squared-img' src='data:image/png;base64,'></image>
        <text class='squared-info'>WIFI密码</text>
      </navigator>
    </view>
    <view class='squared-item'>
      <navigator url='../CouponLists/CouponLists'>
        <image class='squared-img' src='data:image/png;base64,'></image>
        <text class='squared-info'>优惠券</text>
      </navigator>
    </view>
    <view class='squared-item'>
      <navigator url='../StorageValue/StorageValue'>
        <image class='squared-img' src='data:image/png;base64,'></image>
        <text class='squared-info'>储值</text>
      </navigator>
    </view>
    <view class='squared-item'>
      <image class='squared-img' src='data:image/png;base64,'></image>
      <text class='squared-info'>预约打扫</text>
    </view>
    <view class='squared-item'>
      <image class='squared-img' src='data:image/png;base64,'></image>
      <text class='squared-info'>我的合同</text>
    </view>
    <view class='squared-item'>
      <navigator url='../ContactsPersons/ContactsPersons'>
        <image class='squared-img' src='data:image/png;base64,'></image>
        <text class='squared-info'>入住人信息</text>
      </navigator>
    </view>
    <view class='squared-item'>
      <image class='squared-img face' src='data:image/png;base64,'></image>
      <text class='squared-info'>在线客服</text>
    </view>
    <view class='squared-item'>
      <image class='squared-img' src='data:image/png;base64,'></image>
      <text class='squared-info'>发票信息</text>
    </view>
    <view class='squared-item'>
      <image class='squared-img' src='data:image/png;base64,'></image>
      <text class='squared-info'>更多服务</text>
    </view>
  </view>
</view>