.box {
  position: absolute;
  background: #fafafa;
  height: 100%;
  width: 100%;
}

.item {
  height: 120rpx;
  line-height: 120rpx;
  color: #444;
  font-size: 30rpx;
  padding: 0 30rpx;
  border-bottom: 1rpx solid #ddd;
  background: #fff;
  position: relative;
}

.icon {
  vertical-align: middle;
  margin-right: 20rpx;
  margin-top: -6rpx;
}

.headimg {
  width: 38rpx;
  height: 32rpx;
  background: url(data:image/png;base64,);
  background-size: cover;
}

.sub-name {
  width: 32rpx;
  height: 32rpx;
  background: url(data:image/png;base64,);
  background-size: cover;
}

.sex-icon {
  width: 36rpx;
  height: 34rpx;
  background: url(data:image/png;base64,);
  background-size: cover;
}

.phone-icon {
  width: 30rpx;
  height: 40rpx;
  background: url(data:image/png;base64,);
  background-size: cover;
}

.birthday {
  width: 40rpx;
  height: 38rpx;
  background: url(data:image/png;base64,);
  background-size: cover;
}

.item.active::after {
  height: 14rpx;
  width: 14rpx;
  border-top: 1rpx solid #999;
  border-right: 1rpx solid #999;
  transform: rotate(45deg);
  content: "";
  position: absolute;
  right: 30rpx;
  top: 54rpx;
}

.item.active {
  padding-right: 50rpx;
}

.login-out {
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  margin-top: 60rpx;
  border-top: 1rpx solid #ddd;
  color: #f64;
}

.img-icon {
  height: 72rpx;
  width: 72rpx;
  border-radius: 36rpx;
  background: #ddd;
  margin-top: 24rpx;
}
