
 .header-box{
   position: relative;
   display: inline-block;
   font-size: 32rpx;
   margin: 25rpx  242rpx 0;
   height: 378rpx;
 }
.cir {
  display: inline-block;
  background-color: transparent;
  border-radius: 100%;
}
.main-cir {
  border-radius: 50%;
  background-color: rgb(255, 255, 255);
  box-shadow:inset 0rpx -3rpx 6rpx 1.1rpx rgba(0, 0, 0, 0.2);
  position: absolute;
  width: 239rpx;
  height: 239rpx;
  z-index: 46;
  left: 14rpx;
  top: 14rpx;
}
.number{
  position: absolute;
  width: 100%;
  height: 266rpx;
  line-height: 266rpx;
  text-align: center;
  top: 0;
  left: 0;
  z-index: 999;
}
#number{
  font-size: 78rpx;
  color: #ff6644;
}
.header-img{
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 378rpx;
  z-index: -1;
}
.footer{
  background: #ffc441;
  margin-top: -80rpx;
  border-radius: 30rpx 30rpx 0 0;
  position: fixed;
  bottom: 0;
  width: 100%;
  padding: 0 35rpx;
  box-sizing: border-box;
}
.title{
  height: 168rpx;
  line-height: 168rpx;
  font-size: 50rpx;
  color: #fff;
}
.sub-title{
  font-size: 36rpx;
  color: #333;
  margin:40rpx 0 20rpx;
}
.content{
  height: 722rpx;
  position: absolute;
  bottom: 0;
  background: #fff;
  border-radius: 30rpx 30rpx 0 0;
  width: 680rpx;
  padding: 30rpx 20rpx;
  box-sizing: border-box;
  font-size: 24rpx;
  color: #999;
}
.sub-content{
  line-height: 35rpx;
}
.sub-content>view{
position: relative;
padding-left: 40rpx;
margin-bottom: 20rpx;
}
.sub-content>view::before{
  position: absolute;
  content: "";
  width: 26rpx;
  height: 26rpx;
  background: #999;
  border-radius: 13rpx;
  left: 0;
  top: 4rpx;
}
.btn-exchage{
  width: 550rpx;
  height: 86rpx;
  background: #ff6644;
  border-radius: 8rpx;
  position: fixed;
  left: 100rpx;
  bottom: 30rpx;
  color: #fff;
  font-size: 32rpx;
  line-height: 86rpx;
text-align: center;
}
.img-more{
  width: 720rpx;
  height: 332rpx;
  position: absolute;
  left: -15rpx;
  top: -100rpx;
}