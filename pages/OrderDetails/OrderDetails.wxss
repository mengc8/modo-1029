.width100{
width: 100%;
box-sizing: border-box;
}
.border1{
  border-bottom: 1rpx solid #ddd;
}
.container{
  font-size: 26rpx;
  color: #666;
}
.details-header,.details-price,.phone,.btn-box{
  padding: 30rpx;
  display: flex;
  justify-content: space-between;
}
.order-status,.price1{
  font-size: 38rpx;
  color: #ff0000;
}
.time{
  color: #ff6644;
  font-size: 30rpx;
}
.details-price,.phone{
  padding: 20rpx 30rpx;
}
.top-price{
  margin-bottom: 4rpx;
}
.total-price{
  font-size: 24rpx;
}
.mingxi{
  padding-top:48rpx; 
  font-size: 32rpx;
  color: #ff6644;
}
.detail-body{
  padding: 0 30rpx 10rpx;
}
.branch-name{
  font-size: 30rpx;
  line-height: 78rpx;
}
.type,.date{
  line-height: 42rpx;
}
.position{
  padding: 16rpx 30rpx;
}
.phone{
  font-size: 28rpx;
}
.btn-box{
  background: #f2f2f2;
  padding: 10rpx 30rpx;
}
.btn{
  width: 326rpx;
  height: 84rpx;
  border-radius: 8rpx;
  line-height: 82rpx;
  font-size: 30rpx;
  border: 1rpx solid #ddd;
  box-shadow: none;
}
.btn:hover{
background: linear-gradient(to right bottom,#ff0000,#ff6644);
color: #fff;
}
.user-box{
  padding: 10rpx 30rpx;
  line-height: 44rpx;
  border-top: 14rpx solid #f2f2f2;
}
.user-title{
  font-size: 30rpx;
  padding-bottom: 10rpx;
}
.rule{
  border-top: 14rpx solid #f2f2f2;
  padding: 0rpx 30rpx;
  line-height: 32rpx;
  font-size: 22rpx;
  color: #9e9e9e;
}
.rule-title{
  line-height: 80rpx;
  color: #333;
}
.icon-home,.icon-time,.icon-position,.icon-phone{
  width: 28rpx;
  height: 28rpx;
  background: url("data:image/png;base64,");
  background-size: cover;
  vertical-align: middle;
  margin-top: -10rpx
}
.icon-time{
  background: url(data:image/png;base64,);
  background-size: cover;
  vertical-align: text-bottom;
  margin-top: -14rpx;
}
.icon-position{
  background: url(data:image/png;base64,);
  background-size: cover;
}
.icon-phone{
background: url("data:image/png;base64,");
background-size: cover;
margin-top: -8rpx;
}
.toast{
  position: fixed;
  width: 100%;
  height: 100%;
  left: 0;
  top: 0;
  background: rgba(0, 0, 0, 0.4);

}
#mingxi{
  background: #fff;
  position: absolute;
  bottom: 0rpx;
  width: 100%;
  padding: 30rpx;
  box-sizing: border-box;
  font-size: 28rpx;
  color: #666;
  border-top-left-radius: 20rpx;
  border-top-right-radius: 20rpx;
}
.close{
float: right;
width: 60rpx;
height: 60rpx;
text-align: right;
font-size: 34rpx;
}
.toast-title{
font-size: 32rpx;
padding: 40rpx 0 10rpx;
border-bottom: 1rpx solid #eee;
}
.toast-lists{
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
  padding: 40rpx 0 10rpx;
  border-bottom: 1rpx solid #eee;
}
.toast-total{
  float: right;
  margin-top: 30rpx;
}
.colorred{
  color: #ff0000;
}
.total-colorred{
  font-size: 34rpx;
}