.border15{
  border-bottom: 16rpx solid #f2f2f2!important;
}
.room-branch,.room-num-box,.quan-box,.order-box,.huangjin-box,.note-box,.rule-box{
  width: 100%;
  box-sizing: border-box;
  color: #666;
  border-bottom: 1rpx solid #ddd;
}
.room-branch{
  padding:20rpx 30rpx;
}
.room-num-box{
  padding:42rpx 30rpx;
  font-size: 28rpx;
}
.branch-name{
  font-size: 32rpx;
  margin-bottom: 4rpx;
}
.room-type{
  font-size: 26rpx;
}
.room-num-box{
  display: flex;
}
#room_num{
border: 1rpx solid #ddd;
height: 36rpx;
width: 78rpx;
border-radius: 4rpx;
margin:0 58rpx 0 28rpx;
text-align: center;
line-height: 34rpx;
position: relative;
}
#totle_day{
  font-size: 24rpx;
  color: #999;
  margin: 4rpx 0 0 26rpx;
}
.icon-down{
  background: url("data:image/png;base64,");
  width: 19rpx;
  height: 13rpx;
  background-size: cover;
  position: absolute;
  top:12rpx;
  right: 6rpx;
}
.num-box{
  position: absolute;
  left: -1rpx;
  border: 1rpx solid #ddd;
  width: 100%;
  background: #fff;
}
.num-item{
  height: 36rpx;
  border-bottom: 1rpx solid #ddd;
  line-height: 36rpx;
}
.num-item:hover{
  color: #ff6644;
  background: #ddd;
}
.num-item:last-of-type{
  border-bottom: none;
}
.quan-box,.order-box,.huangjin-box,.note-box{
  display: flex;
  justify-content: space-between;
}
.quan-box{
  padding: 30rpx 30rpx;
}
.icon-quan{
  width: 28rpx;
  height: 36rpx;
  background: url("data:image/png;base64,");
  background-size: cover;
  vertical-align: middle;
  margin: -8rpx 10rpx 0;
}
.order-box{
  padding: 36rpx 30rpx;
}
.huangjin-box{
  padding: 22rpx 30rpx;
}
.vip-more{
  margin-top: 12rpx;
  font-size: 20rpx;
  position: relative;
}
.vip-more::after{
  display: inline-block;
  position: absolute;
  height: 24rpx;
  width: 24rpx;
  border-radius: 13rpx;
  border: 2rpx solid #ff6644;
  content: "ï¼Ÿ";
  color: #ff6644;
  text-align: center;
  font-weight: 600;
  left: 170rpx;
  box-sizing: border-box;
  padding-left: 4rpx;
  line-height: 20rpx;
 top: 2rpx;
}
.vip-price{
  line-height: 84rpx;
  position: relative;
  margin-right: 46rpx;
}
.chose-vip,.no-chose-vip{
  height: 28rpx;
  width: 28rpx;
  border: 2rpx solid #333;
  border-radius: 16rpx;
  position: absolute;
  top: 28rpx;
  right: -40rpx;
}
.no-chose-vip{
  border: 2rpx solid #ff6644;
}
.no-chose-vip::before,.no-chose-vip::after{
  height: 2rpx;
  width: 18rpx;
  position: absolute;
  display: inline-block;
  content: "";
  background: #ff6644;
  top:13rpx;
  left: 10rpx;
  transform: rotate(-50deg);
}
.no-chose-vip::after{
  transform: rotate(40deg);
  width: 8rpx;
  top: 17rpx;
  left: 6rpx;
}
.note-box,.rule-box{
  padding: 25rpx 30rpx;
}
#note{
    width: 600rpx;
    vertical-align: middle;
    margin-top: -4rpx;
}
.rule-box{
  padding-bottom: 126rpx;
}
.rule-title{
  margin-bottom: 30rpx;
}
.rule-content{
color: #999;
font-size: 22rpx;
line-height: 34rpx;
}
.btn-box{
  position: fixed;
  bottom: 0;
  width: 100%;
  height: 116rpx;
  background: #fff;
  line-height: 116rpx;
  border-top: 1rpx solid #ddd;
  text-indent: 30rpx;
  color: #ff0000;
  font-size: 44rpx;
}
.order-btn{
  height: 100%;
  width: 220rpx;
  color: #fff;
  background: linear-gradient(to right bottom,#ff0000,#ff6644);
  float: right;
  border-radius: 0;
  line-height: 116rpx;
}
.yuan{
  font-size: 24rpx;
}